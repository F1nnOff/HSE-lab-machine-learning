    "import copy\n",
    "\n",
    "b = np.ones(n)\n",
    "\n",
    "#многочлены 1-ой степени для train\n",
    "Xtra = np.column_stack((X_train, b))\n",
    "\n",
    "def stepen(matr, n):\n",
    "    matr1 = copy.copy(matr);\n",
    "    for i in range(len(matr)):\n",
    "        matr1[i] = matr1[i] ** n;\n",
    "    return matr1;\n",
    "\n",
    "\n",
    "def polynom(matr, n, base):\n",
    "    count = 2;\n",
    "    while count <= n:\n",
